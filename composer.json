{
	"name": "symbiote-library/silverstripe-s3publisher",
	"description": "A static publisher which publishes the site to an S3 bucket",
	"type": "silverstripe-module",
	"keywords": ["silverstripe", "s3", "static"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Andrew Short",
			"email": "andrew@symbiote.com.au"
		}
	],
	"require": {
		"aws/aws-sdk-php": "2.*",
		"silverstripe/staticpublishqueue": "*",
		"symfony/finder": "2.4.*"
	},
	"autoload": {
		"psr-4": {
			"Symbiote\\S3Publisher": "src"
		}
	},
	"extra": {
		"installer-name": "s3publisher",
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	},
	"replace": {
		"silverstripe-australia/s3publisher": "self.version"
	}
}
